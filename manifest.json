{
  "manifest_version": 2,
  "name": "Web Time Analytics",
  "description": "Helps with data visualization based on time spent on specific web pages.",
  "version": "1.0",
  "icons": {
	"16": "icon16.png",
	"48": "icon48.png",
	"128": "icon128.png"
  },
  "browser_action": {
	"default_icon": "icon48.png",
	"default_popup": "popup.html",
	"default_title": "See Web Time Usage."
  },
  "background": {
	"scripts": [
	  "uuid.js",
	  "jquery-3.1.0.min.js",
	  "background.js"
	]
  },
  "permissions": [
	"activeTab",
	"identity",
	"storage",
	"tabs",
	"idle"
  ]
}