{
    "manifest_version": 2,
    "name": "Web Time Analytics",
    "description": "Helps with data visualization based on time spent on specific web pages.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "See Web Time Usage."
    },
    "background": {
        "scripts": [
            "uuid.js",
            "jquery-3.1.0.min.js",
            "background.js"
        ]
    },
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "content.js"
        ]
    }],
    "permissions": [
        "activeTab",
        "identity",
        "storage",
        "tabs"
    ]
}